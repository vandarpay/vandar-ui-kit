{
  "name": "vandar-ui-kit",
  "private": false,
  "version": "0.0.29",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "lint:js": "eslint src --ext .ts,.vue",
    "dev": "vite",
    "build": "vue-tsc && vite build && yarn build:styles && yarn build:twPresets && yarn build:fonts",
    "build:twPresets": "mkdir ./dist/twPresets && cp -r ./designTokens ./dist/twPresets && cp tailwind.config.cjs ./dist/twPresets/twPresets.cjs",
    "build:fonts": "mkdir ./dist/fonts && cp -r ./src/assets/fonts ./dist/",
    "build:styles": "postcss src/assets/tailwind.css -o dist/index.css",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "remixicon": "^2.5.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.15",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.0",
    "vue-eslint-parser": "^9.1.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.11"
  }
}
